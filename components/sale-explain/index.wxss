.sleeve{
    font-family: zh_mall !important;
    color: #157658 !important;
    margin-top: 5rpx;
    margin-right: 26rpx;
}

.container{
    box-sizing: border-box;
    width: 710rpx;
    border: 2rpx dashed #dcede6;
    display: flex;
    flex-direction: column;
    padding: 5rpx 25rpx;
    background-color: #ffffff;
}

.inner{
    height: 50rpx;
    border-bottom: 2rpx solid #dcede6;
    display: flex;
    flex-direction: row;
    justify-content: start;
    font-size: 24rpx;
    color: #333333;
}

.inner text{
    margin-top: 8rpx;
}

